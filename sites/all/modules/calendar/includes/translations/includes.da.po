msgid "Edit"
msgstr "Redigér"

#: includes/calendar_view_plugin_style.inc:87
msgid "Custom"
msgstr "Tilpasset"

#: includes/calendar_view_plugin_style.inc:87
msgid "Hour"
msgstr "Time"

#: includes/calendar_plugin_display_page.inc:139
